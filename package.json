{
    "name": "web3-sekit",
    "version": "2.0.0",
    "author": "devilyouwei",
    "license": "MIT",
    "description": "Automated software engineering kits for Web3, Blockchain and Smart Contracts.",
    "main": "web/app.js",
    "scripts": {
        "format": "prettier 'src/*.js' --write",
        "clean": "rm -rf ./tf/logs/**/train ./tf/mymodels/**/*",
        "DB": "node ./db/initDB.js",
        "contract": "node ./db/contract.js",
        "token": "node ./db/token.js",
        "data": "node ./db/data.js",
        "vulnerability": "node ./db/vulnerability.js",
        "updateContract": "node ./src/contract.js",
        "updateToken": "node ./src/token.js",
        "updateVulnerability": "node ./src/vulnerability.js",
        "scamsniper": "node ./src/scamsniper.js",
        "safu": "node ./src/safu.js",
        "tokenizer": "node ./tensorflow/tokenizer.js",
        "dev": "nodemon ./web/app.js",
        "restart": "pm2 restart ./web/app.js",
        "start": "pm2 start ./web/app.js",
        "stop": "pm2 stop ./web/app.js",
        "status": "pm2 status",
        "delete": "pm2 delete app",
        "pm2-update": "pm2 update",
        "highlight": "node ./tensorflow/modules/highlight.js",
        "dense": "node ./tensorflow/dense.js",
        "smartbert-dense": "node ./tensorflow/smartbert-dense.js",
        "cnn": "node ./tensorflow/cnn.js",
        "smartbert-cnn": "node ./tensorflow/smartbert-cnn.js",
        "lstm": "node ./tensorflow/lstm.js",
        "bilstm": "node ./tensorflow/bilstm.js",
        "smartbert-lstm": "node ./tensorflow/smartbert-lstm.js",
        "smartbert-high-lstm": "node ./tensorflow/smartbert-high-lstm.js",
        "smartbert-high-bilstm": "node ./tensorflow/smartbert-high-bilstm.js",
        "smartbert-bilstm": "node ./tensorflow/smartbert-bilstm.js",
        "smartbert-dense-lstm": "node ./tensorflow/smartbert-dense-lstm.js",
        "smartbert-dense-bilstm": "node ./tensorflow/smartbert-dense-bilstm.js",
        "smartbert-concat-lstm": "node ./tensorflow/smartbert-concat-lstm.js",
        "smartbert-concat-bilstm": "node ./tensorflow/smartbert-concat-bilstm.js",
        "mysql": "docker-compose -f ./docker/docker-compose.yml up -d",
        "ncu": "ncu -u"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/decentralizedlab/web3-sekit"
    },
    "keywords": [
        "web3",
        "blockchain",
        "smart contract",
        "crawler",
        "software engineering",
        "AI",
        "deep learning"
    ],
    "bugs": {
        "url": "https://github.com/decentralizedlab/web3-sekit/issues"
    },
    "homepage": "http://web3.cas-ll.cn",
    "devDependencies": {
        "@types/serve-index": "^1.9.1",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "nodemon": "^3.1.4",
        "npm-check-updates": "^16.6.3",
        "prettier": "^2.8.3"
    },
    "dependencies": {
        "@stdlib/assert-is-json": "^0.1.1",
        "@tensorflow/tfjs-node": "^4.20.0",
        "@tensorflow/tfjs-node-gpu": "^4.20.0",
        "app-root-path": "^3.1.0",
        "axios": "^1.5.1",
        "bert-tokenizer": "^1.1.8",
        "body-parser": "^1.20.1",
        "cheerio": "^1.0.0-rc.12",
        "dotenv": "^16.0.3",
        "eventsource-parser": "^1.1.1",
        "express": "^4.18.2",
        "form-data": "^4.0.0",
        "lodash": "^4.17.21",
        "md5": "^2.3.0",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.1.0",
        "node-fetch": "2",
        "node-xlsx": "^0.21.0",
        "pm2": "^5.2.2",
        "puppeteer": "^19.6.3",
        "puppeteer-extra": "^3.3.4",
        "puppeteer-extra-plugin-adblocker": "^2.13.5",
        "puppeteer-extra-plugin-stealth": "^2.11.1",
        "request": "^2.88.2",
        "sentencepiece-js": "^1.1.0",
        "sequelize": "^6.28.0",
        "serve-index": "^1.9.1",
        "solc": "latest",
        "superagent": "^8.0.9",
        "tf-kmeans-node": "^1.1.1",
        "treebank-tokenizer": "^0.0.1",
        "treeify": "^1.1.0",
        "web3": "^1.8.2"
    },
    "eslintConfig": {
        "root": true,
        "parserOptions": {
            "ecmaVersion": "latest"
        },
        "env": {
            "es6": true,
            "node": true
        },
        "extends": [
            "eslint:recommended",
            "plugin:prettier/recommended"
        ],
        "rules": {
            "no-var": "error",
            "no-unused-vars": "warn",
            "prefer-const": "error",
            "no-invalid-regexp": "off",
            "no-div-regex": "off",
            "no-control-regex": "off",
            "prettier/prettier": "error"
        }
    },
    "prettier": {
        "trailingComma": "none",
        "printWidth": 120,
        "tabWidth": 4,
        "semi": false,
        "bracketSpacing": true,
        "arrowParens": "avoid",
        "singleQuote": true
    }
}
